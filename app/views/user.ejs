	});
</script>
<script type="text/javascript"> 
$(document).ready(function(){
	$("#youSay").keydown(function(){
		var curLength=$("#youSay").val().length;	
		if(curLength>=200){
			var num=$("#youSay").val().substr(0,999);
			$("#youSay").val(num);
			alert("字数超过限制啦、多出的字会没有的、有想说的见面说哦" );
		}
		else{
			$("#textCount").text(199-$("#youSay").val().length)
		}
	})
})
</script>
