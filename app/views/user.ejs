						<div class="col-md-6 col-sm-6">
							<input class="form-control required" type="text" name="college" placeholder="学院">
						</div>
						<div class="col-md-6 col-sm-6">
							<input class="form-control required" type="text" name="classes" placeholder="班级">
						</div>
						<div class="col-md-6 col-sm-6">
							<input class="form-control required" type="text" name="stuNum" placeholder="学号">
						</div>						
						<div class="col-md-6 col-sm-6">
							<input class="form-control required" type="text" name="tel" placeholder="电话">
						</div>
						<div class="col-md-6 col-sm-6">
							<input class="form-control required" type="text" name="qq" placeholder="QQ">
						</div>
						<div class="col-md-6 col-sm-6">
							<input class="form-control required" type="text" name="desire" placeholder="你的意向？前端/开发/服务平台/企划/网络维护">
						</div>
						<div class="col-md-12 col-sm-12">
							<textarea id="youSay" class="form-control" name="reason" placeholder="让我了解你的爱好/特长/申请加入的理由" rows="6"></textarea>
							<p style="padding-top:0; text-align:right">还可输入<span id="textCount">200</span>字</p>
						</div>
						<div class="col-md-offset-2 col-md-8 col-sm-offset-2 col-sm-8">
							<input class="form-control submit" type="submit" value="提交">
						</div>
